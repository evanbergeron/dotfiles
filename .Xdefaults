URxvt.depth: 32

URxvt.saveLines:32767
URxvt.scrollBar: false

! URxvt*font: xft:Inconsolatas:pixelsize=16
! URxvt.letterSpace: 1

URxvt.xftAntialias:     true
URxvt.font:             xft:inconsolata:pixelsize=20:antialias=true:hinting=true, xft:dejavu sans mono:pixelsize=20:antialias=true,hinting=true
URxvt.boldFont:         xft:inconsolata:pixelsize=20:antialias=true:hinting=true, xft:dejavu sans mono:pixelsize=20:antialias=true,hinting=true
URxvt.italicFont:       xft:inconsolata:pixelsize=20:antialias=true:hinting=true, xft:dejavu sans mono:pixelsize=20:antialias=true,hinting=true
URxvt.bolditalicFont:   xft:inconsolata:pixelsize=20:antialias=true:hinting=true, xft:dejavu sans mono:pixelsize=20:antialias=true,hinting=true

Xft.dpi: 150
Xft.antialias: true

! Pseudo Transparency
! URxvt*shading: 20
! URxvt*transparent:true

URxvt.perl-ext-common: default,matcher
URxvt.urlLauncher: uzbl-browser
URxvt.matcher.button: 2
URxvt.matcher.pattern.1: \\bwww\\.[\\w-]\\.[\\w./?&@#-]*[\\w/-]

!!URxvt Color Scheme - Spectre
*background: #232c31
*foreground: #D2C5BC

! BLACK
*color0:  #525252
*color8:  #707070
! RED
*color1:  #B0777B
*color9:  #AA7E87
! GREEN
*color2:  #94A65D
*color10: #BACD8F
! YELLOW
*color3:  #E8C98E
*color11: #E0B274
! BLUE
*color4:  #869AA3
*color12: #96A8B7
! MAGENTA
*color5:  #9F98B7
*color13: #A393CE
! CYAN
*color6:  #93A2B9
*color14: #9BB7CC
! WHITE
*color7:  #D2D1D4
*color15: #D6DAC3
